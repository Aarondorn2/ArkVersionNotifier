<queue-entries>
<!--Change the refresh rate of the default queue to 1/s-->
  <queue>
    <name>confirmationQueue</name>
    <rate>1/s</rate>
    <retry-parameters>
      <task-retry-limit>3</task-retry-limit>
      <min-backoff-seconds>60</min-backoff-seconds>
    </retry-parameters>
  </queue>
</queue-entries>